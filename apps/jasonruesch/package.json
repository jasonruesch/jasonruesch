{
  "name": "@jasonruesch/jasonruesch",
  "private": true,
  "type": "module",
  "scripts": {},
  "sideEffects": false,
  "dependencies": {
    "@react-router/node": "^7.2.0",
    "@react-router/serve": "^7.2.0",
    "isbot": "^4.4.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-router": "^7.2.0",
    "tailwindcss": "^4.1.10",
    "react-ga4": "^2.1.0"
  },
  "devDependencies": {
    "@react-router/dev": "^7.2.0",
    "@types/node": "^20.0.0",
    "@types/react": "19.0.0",
    "@types/react-dom": "19.0.0"
  },
  "nx": {
    "targets": {
      "docker-build": {
        "dependsOn": [
          "build"
        ],
        "command": "docker build -f apps/jasonruesch/Dockerfile . -t jasonruesch"
      },
      "deploy": {
        "command": "flyctl deploy --config apps/jasonruesch/fly.toml",
        "configurations": {
          "production": {
            "command": "flyctl deploy --config apps/jasonruesch/fly.toml"
          },
          "staging": {
            "command": "flyctl deploy --config apps/jasonruesch/fly.staging.toml"
          }
        }
      }
    }
  },
  "version": "2.1.0"
}
